<html>
  <header>
    <link rel="stylesheet" href="stylesheets/bootstrap.css" />
  </header>
  <body>
    <div class="container ">
      <h1>Login Page Student</h1>

      <form class="for" action="/login" method="post">
        <div class="form-group">
          <label for="id"> EmpId </label>
          <input class="form-control" type="text" id="id" name="emp[empId]" />
        </div>
        <div class="form-group">
          <label for="pass">Password</label>
          <input
            class="form-control"
            type="text"
            id="pass"
            name="emp[password]"
          />
        </div>
        <div class="form-group">
          <input type="submit" class="btn btn-primary" />
        </div>
      </form>
    </div>
  </body>
</html>
